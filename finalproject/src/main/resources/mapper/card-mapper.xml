<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.kopo.finalproject.Card.model.dao.CardMapper">
    <select id="getRegisteredCardByMemberID" parameterType="String" resultType="com.kopo.finalproject.Card.model.dto.Card">
        SELECT *
        FROM RegisteredCard
        WHERE MemberID = #{memberID}
    </select>
</mapper>

